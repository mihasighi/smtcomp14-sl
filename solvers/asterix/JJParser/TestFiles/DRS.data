%----All cats are furry.
drs([],[drs([A],[object(A,cat,countable,na,eq,1) - 1/2])=>drs([B,C],[property(B,furry,pos)-1/4,predicate(C,be,A,B)-1/3])])

%----John enters a card.
drs([A,B],[object(A,card,countable,na,eq,1)-1/4,predicate(B,enter,named('John'),A)-1/2])

%----A member of the n:Curie-family wins 2 prizes.
drs([A,B,C,D],[object(A,member,countable,na,eq,1)-1/2,object(B,'Curie-family',countable,na,eq,1)-1/5,object(C,prize,countable,na,eq,2)-1/8,relation(A,of,B)-1/3,predicate(D,win,A,C)-1/6])

