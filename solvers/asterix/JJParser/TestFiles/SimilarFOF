fof(freddy,axiom,
    ( ! [X,Y] : 
        ( q(X)
       => ( ~ p1(X)
          & (f(X,cat) = X => X != god)
          & ($false | $true)
          & p2(X,h(X,cat,g(cat))) ) ) )).

fof(freddy,axiom,
    ( ! [X,Z] : 
        ( q(X)
       => ( ~ p1(X)
          & (X = f(X,cat) => X != god)
          & ($false | $true)
          & p2(X,h(X,cat,g(cat))) ) ) )).

