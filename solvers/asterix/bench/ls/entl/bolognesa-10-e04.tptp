%====Include axioms for Lists in Separation Logic
include('Axioms/SepLogicLists.ax').

cnf(premise_1,hypothesis,
    ( heap(sep(next(x3, x7), sep(next(x1, x4), sep(next(x4, x8), sep(ls(x8, x3), sep(next(x2, x7), sep(next(x5, x3), sep(next(x6, x8), sep(ls(x9, x4), sep(next(x10, x7), sep(ls(x7, x9), emp))))))))))) )).

cnf(conclusion_1,negated_conjecture,
    ( ~ heap(sep(ls(x2, x7), sep(ls(x6, x8), sep(ls(x5, x3), sep(ls(x10, x7), sep(ls(x9, x4), sep(ls(x1, x4), sep(ls(x4, x9), emp)))))))) )).

