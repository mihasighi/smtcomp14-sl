cnf(premise_1,hypothesis,
    ( heap(sep(ls(x4, x6), sep(next(x9, x12), sep(next(x5, x7), sep(ls(x7, x9), sep(next(x13, x8), sep(next(x8, x1), sep(ls(x6, x5), sep(ls(x11, x13), sep(next(x3, x8), sep(next(x10, x3), sep(next(x12, x1), sep(next(x2, x7), sep(next(x1, x11), emp)))))))))))))) )).

